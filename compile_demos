docker run --rm \
    --volume="$(pwd):/root/demos:rw" \
    cuda4opencv/demos:ros-kinetic \
    /bin/bash -c "mkdir -p build && cd build && cmake .. && make"

docker run --rm \
    --volume="$(pwd):/root/demos:rw" \
    cuda4opencv/demos:ros-kinetic \
    /bin/bash -c "cd ros && catkin_make -DOpenCV_DIR=/usr/local/share/OpenCV"

